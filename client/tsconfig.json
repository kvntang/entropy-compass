{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "dom"],
    "paths": {
      "@/*": ["./*"]
    },
    "outDir": "../dist/client",
    "types": ["@types/p5"] // Add this line
  },
  "include": ["./**/*.ts", "./**/*.vue"]
}